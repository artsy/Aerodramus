source 'https://github.com/CocoaPods/Specs.git'
use_frameworks!
platform :ios, '8.0'

plugin 'cocoapods-keys', {
    :project => "Aerodramus",
    :target => "Aerodramus_Example",
    :keys => ["EchoKey"]
}

target 'Aerodramus_Example', :exclusive => true do
  pod "Aerodramus", :path => "../"
end

target 'Aerodramus_Tests', :exclusive => true do
  pod "Aerodramus", :path => "../"

  pod 'Specta'
  pod 'Expecta'
end
